---
title: Fixtures
---

<div class="container">
	<div class="row">
		<div class="col-md-8">
			<h2>Club Fixtures for 2016-17 Season</h2>
			<table class="table table-striped table-hover ">
				<thead>
					<tr>
						<th>Date</th>
						<th>Competition</th>
						<th>Opponents</th>
						<th>Home or Away?</th>
					</tr>
				</thead>
				<tbody>
				<% data.fixtures.fixtures.each do |fixture| %>
					<tr>
						<td><%= fixture.when %></td>
						<td><%= fixture.competition %></td>
						<td><%= fixture.against %></td>
						<td><%= fixture.venue %></td>
					</tr>
				<% end %>
				</tbody>
			</table>
		</div>
		<div class="col-md-4">
			<%= partial "chesslinks" %>
		</div>
	</div>
</div>
